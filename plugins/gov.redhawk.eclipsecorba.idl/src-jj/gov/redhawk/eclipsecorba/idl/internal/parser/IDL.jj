/* Definitions of complex regular expressions follow */
TOKEN :
{
  < ID : [ "a"-"z", "A"-"Z", "_" ] ([ "a"-"z", "A"-"Z", "0"-"9", "_" ])* >
| < OCTALINT : "0" ([ "0"-"7" ])* ([ "u", "U", "l", "L" ])? >
| < DECIMALINT : [ "1"-"9" ] ([ "0"-"9" ])* ([ "u", "U", "l", "L" ])? >
| < FIXED_PT :
    (
      [ "1"-"9" ] ([ "0"-"9" ])*
    )
    (
      "." [ "1"-"9" ] ([ "0"-"9" ])*
    )?
    ([ "d", "D" ]) >
| < HEXADECIMALINT :
    (
      "0x"
    | "0X"
    )
    ([ "0"-"9", "a"-"f", "A"-"F" ])+ ([ "u", "U", "l", "L" ])? >
| < FLOATONE :
    (
      ([ "0"-"9" ])+ "." ([ "0"-"9" ])*
    | ([ "0"-"9" ])* "." ([ "0"-"9" ])+
    )
    (
      [ "e", "E" ] ([ "-", "+" ])? ([ "0"-"9" ])+
    )?
    ([ "f", "F", "l", "L" ])? >
| < FLOATTWO : ([ "0"-"9" ])+ [ "e", "E" ] ([ "-", "+" ])? ([ "0"-"9" ])+ ([ "f", "F", "l", "L" ])? >
| < CHARACTER :
    "'"
    (
      (~[ "'", "\\", "\n", "\r" ])
    |
      (
        "\\"
        (
          [ "n", "t", "v", "b", "r", "f", "a", "\\", "?", "'", "\"" ]
        | "0" ([ "0"-"7" ])*
        | [ "1"-"9" ] ([ "0"-"9" ])*
        |
          (
            "0x"
          | "0X"
          )
          ([ "0"-"9", "a"-"f", "A"-"F" ])+
        )
      )
    )
    "'" >
| < WCHARACTER :
    "L" "'"
    (
      (~[ "'", "\\", "\n", "\r" ])
    |
      (
        "\\"
        (
          [ "n", "t", "v", "b", "r", "f", "a", "\\", "?", "'", "\"" ]
        | "0" ([ "0"-"7" ])*
        | [ "1"-"9" ] ([ "0"-"9" ])*
        |
